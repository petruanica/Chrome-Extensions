{
  "name": "Covid Tracker",
  "description": "Tracks new Covid cases and deaths in the chosen country",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "javascripts/background.js"
  },
  "permissions": ["storage"],
  "options_page": "layouts/options.html",
  "action": {
    "default_popup": "layouts/popup.html",
    "default_title": "Latest Covid Report"
  }
}
